export interface IConflictGenerator {
  getId(): number;
  processData(): Promise<string>;
}

export class ConflictGenerator implements IConflictGenerator {
  private id: number;

  constructor(id: number) {
    this.id = id;
  }

  public getId(): number {
    return this.id;
  }

  public async processData(): Promise<string> {
    return `Processed data for ID ${this.id} from bc-3 branch`;
  }
}
